# NOTE: devDB
alias pgdev='gcloud compute ssh --tunnel-through-iap --zone "asia-northeast1-a" --project "$PROJECT_DEV" $BASTION'
# NOTE: ローカルDB
alias pglocal='psql -h localhost -p $POSTGRES_PORT -U $POSTGRES_USER -d $POSTGRES_DB'

# NOTE: プロジェクト環境変数 for DB
# Service
db() {
    export POSTGRES_PORT=
    export POSTGRES_USER=
    export POSTGRES_DB=
    export PGPASSWORD=
    export PROJECT_DEV=
    export BASTION=
    echo "✅ Switched to ExampleService"
}

aws() {
    export AWS_ACCOUNT_ID=
    export AWS_PROFILE=
}

## setup-env
if [ -f ~/dotfiles/.zshrc.env ]; then
    source ~/dotfiles/.zshrc.env
fi
